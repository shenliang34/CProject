{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "gcc build",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${workspaceFolder}/main.c",
				"-o",
				"${workspaceFolder}/main"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "编译器: /usr/bin/gcc"
		}
	]
}